{
    "build": {
      "installCommand": "npm install",
      "buildCommand": "npm run build"
    },
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ],
    "routes": [
      { "src": "/.*", "dest": "/index.html" }
    ]
  }

  {
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }